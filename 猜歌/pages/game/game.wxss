page {
    height: 100%;
}

.container {
    height: 100%;
}

.game {
    height: 100%;
}

.ready {
    position: absolute;
    top: 40%;
    left: 50%;
    z-index: 3;
    margin-left: -150rpx;
    margin-top: -150rpx;
    width: 300rpx;
    height: 300rpx;
    line-height: 300rpx;
    text-align: center;
    font-size: 100rpx;
    font-weight: 700;
    color: #fff;
    opacity: 0;
    transform: scale(5);
}

.ready-in {
    animation: ready-in 1.2s;
}

@-webkit-keyframes ready-in {
    0% {
        opacity: 0;
        transform: scale(5);
    }

    25% {
        opacity: 1;
        transform: scale(1);
    }

    50% {
        opacity: 1;
        transform: scale(1);
    }

    75% {
        opacity: 1;
        transform: scale(1);
    }

    100% {
        opacity: 0;
        transform: scale(1);
    }
}

@keyframes ready-in {
    0% {
        opacity: 0;
        transform: scale(5);
    }

    25% {
        opacity: 1;
        transform: scale(1);
    }

    50% {
        opacity: 1;
        transform: scale(1);
    }

    75% {
        opacity: 1;
        transform: scale(1);
    }

    100% {
        opacity: 0;
        transform: scale(1);
    }
}

.ready-out {
    display: none;
}

.step {
    position: relative;
    z-index: 10;
    margin: 0 auto;
    width: 406rpx;
    height: 98rpx;
    line-height: 92rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: cover;
    text-align: center;
    font-size: 36rpx;
    font-weight: bold;
    color: #fff;
    opacity: 0;
    transform: translateY(-98rpx);
    transition: all 0.5s;
}

.step-in {
    opacity: 1;
    transform: translateY(0);
}

.box {
    position: relative;
    z-index: 10;
    width: 100%;
    height: 440rpx;
    opacity: 0;
    transition: all 0.5s;
}

.box-in {
    opacity: 1;
}

.box .wave1 {
    position: absolute;
    left: -750rpx;
    top: 50%;
    margin-top: -36rpx;
    width: 1500rpx;
    height: 72rpx;
}

.box .wave1.play {
    animation-name: wava-run1;
    animation-duration: 10s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
}

.box .wave1.stop,.box .wave1.over {
    animation-play-state: paused;
}

.box .wave2 {
    position: absolute;
    right: -750rpx;
    top: 50%;
    margin-top: -36rpx;
    width: 1500rpx;
    height: 72rpx;
}

.box .wave2.play {
    animation-name: wava-run2;
    animation-duration: 10s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
}

.box .wave2.stop,.box .wave2.over {
    animation-play-state: paused;
}

@-webkit-keyframes wava-run1 {
    0% {
        transform: translateX(0);
    }

    50% {
        transform: translateX(750rpx);
    }

    100% {
        transform: translateX(0);
    }
}

@keyframes wava-run1 {
    0% {
        transform: translateX(0);
    }

    50% {
        transform: translateX(750rpx);
    }

    100% {
        transform: translateX(0);
    }
}

@-webkit-keyframes wava-run2 {
    0% {
        transform: translateX(0);
    }

    50% {
        transform: translateX(-750rpx);
    }

    0% {
        transform: translateX(0);
    }
}

@keyframes wava-run2 {
    0% {
        transform: translateX(0);
    }

    50% {
        transform: translateX(-750rpx);
    }

    0% {
        transform: translateX(0);
    }
}

.box .progress {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -190rpx;
    margin-top: -190rpx;
    width: 380rpx;
    height: 380rpx;
    box-shadow: 0 0 10rpx rgba(0,0,0,.2);
    border-radius: 50%;
}

.box .progress .left-block {
    position: absolute;
    left: 0;
    top: 0;
    width: 190rpx;
    height: 380rpx;
    overflow: hidden;
}

.box .progress .right-block {
    position: absolute;
    right: 0;
    top: 0;
    width: 190rpx;
    height: 380rpx;
    overflow: hidden;
}

.box .progress .run-block {
    position: absolute;
    left: 50%;
    top: 0;
    margin-left: -12rpx;
    width: 24rpx;
    height: 380rpx;
}

.box .progress .left-circle {
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    width: 380rpx;
    height: 380rpx;
    border-left: 6rpx solid rgba(241,185,236,1);
    border-top: 6rpx solid rgba(255,255,255,0.3);
    border-right: 6rpx solid rgba(255,255,255,0.3);
    border-bottom: 6rpx solid rgba(241,185,236,1);
    border-radius: 50%;
    transform: rotate(-135deg);
}

.box .progress .right-circle {
    position: absolute;
    right: 0;
    top: 0;
    box-sizing: border-box;
    width: 380rpx;
    height: 380rpx;
    border-left: 6rpx solid rgba(255,255,255,0.3);
    border-top: 6rpx solid rgba(241,185,236,1);
    border-right: 6rpx solid rgba(241,185,236,1);
    border-bottom: 6rpx solid rgba(255,255,255,0.3);
    border-radius: 50%;
    transform: rotate(-135deg);
}

.box .progress .run-circle {
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    margin-top: -9rpx;
    width: 24rpx;
    height: 24rpx;
    background: rgba(241,185,236,1);
    border-radius: 50%;
}

.box .progress.play .left-circle {
    animation-name: progress-left;
    animation-timing-function: linear;
}

.box .progress.stop .left-circle {
    animation-play-state: paused;
}

.box .progress.over .left-circle {
    transform: rotate(45deg);
}

.box .progress.play .right-circle {
    animation-name: progress-right;
    animation-timing-function: linear;
}

.box .progress.stop .right-circle {
    animation-play-state: paused;
}

.box .progress.over .right-circle {
    transform: rotate(45deg);
}

.box .progress.play .run-block {
    animation-name: progress-run;
    animation-timing-function: linear;
}

.box .progress.stop .run-block {
    animation-play-state: paused;
}

.box .progress.over .run-block {
    transform: rotate(360deg);
}

@-webkit-keyframes progress-right {
    0% {
        -webkit-transform: rotate(-135deg);
    }

    50% {
        -webkit-transform: rotate(45deg);
    }

    100% {
        -webkit-transform: rotate(45deg);
    }
}

@keyframes progress-right {
    0% {
        -webkit-transform: rotate(-135deg);
    }

    50% {
        -webkit-transform: rotate(45deg);
    }

    100% {
        -webkit-transform: rotate(45deg);
    }
}

@-webkit-keyframes progress-left {
    0% {
        transform: rotate(-135deg);
    }

    50% {
        transform: rotate(-135deg);
    }

    100% {
        transform: rotate(45deg);
    }
}

@keyframes progress-left {
    0% {
        transform: rotate(-135deg);
    }

    50% {
        transform: rotate(-135deg);
    }

    100% {
        transform: rotate(45deg);
    }
}

@-webkit-keyframes progress-run {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(360deg);
    }
}

@keyframes progress-run {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(360deg);
    }
}

.box .player {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -170rpx;
    margin-top: -170rpx;
    width: 340rpx;
    height: 340rpx;
    border-radius: 50%;
    box-sizing: border-box;
}

.box .player .cp {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -170rpx;
    margin-top: -170rpx;
    width: 340rpx;
    height: 340rpx;
    border-radius: 50%;
    box-shadow: 0 0 20rpx rgba(0,0,0,.3);
}

.box .player.play .cp {
    animation-name: cp-rotate;
    animation-duration: 5s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
}

.box .player.stop .cp {
    animation-play-state: paused;
}

.box .player.none .cp {
}

@-webkit-keyframes cp-rotate {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

@keyframes cp-rotate {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

.box .player .control {
    position: absolute;
    right: -35rpx;
    top: -35rpx;
    width: 124rpx;
    height: 194rpx;
    transform-origin: 100% 0;
    transition: 0.5s;
    transform: rotate(-35deg);
}

.box .player .control.play {
    transform: rotate(-10deg);
}

.box .player .control.stop {
    transform: rotate(-35deg);
}

.box .player .clock {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -60rpx;
    margin-top: -60rpx;
    width: 120rpx;
    height: 120rpx;
    background: rgba(116,1,170,.7);
    border-radius: 50%;
}

.box .player .clock text {
    display: none;
    width: 120rpx;
    height: 120rpx;
    line-height: 120rpx;
    font-size: 56rpx;
    color: #fff;
    text-align: center;
}

.box .player .clock text.hide {
    display: none;
}

.box .player .clock text.show {
    display: block;
}

.answers {
    position: relative;
    z-index: 10;
    display: flex;
    flex-flow: column;
    justify-content: center;
    padding: 0 65rpx;
    height: calc(100% - 640rpx);
    opacity: 0;
    transform: translateY(30%);
    transition: all 0.5s;
}

.answers-in {
    opacity: 1;
    transform: translateY(0);
}

.answers .answer {
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    flex: 1;
    margin: 12.5rpx 0;
    padding: 0;
    max-height: 108rpx;
    background: #fff linear-gradient(to right,#ae63f2,#7679fc);
    border-radius: 100rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.answers .answer[disabled] {
    background: #fff linear-gradient(to right,#aaaaaa,#7679fc);
}

.answers .answer::after {
    border: none;
    border-radius: 0;
}

.answers .answer text {
    padding-left: 60rpx;
    font-size: 36rpx;
    color: #fff;
}

.answers .answer icon {
    content: "";
    margin-right: 30rpx;
    width: 56rpx;
    height: 56rpx;
    background-repeat: no-repeat;
    background-position: 50% 50%;
    background-size: 100% 100%;
}

.answers .answer-none {
    background: #fff linear-gradient(to right,#ae63f2,#7679fc);
}

.answers .answer-select {
    background: #00a4f9;
    box-shadow: 0 5rpx 5rpx rgba(0,164,249,.2);
}

.answers .answer-right {
    background: #65c038!important;
    box-shadow: 0 5rpx 5rpx rgba(101,192,56,.2);
}

.answers .answer-right icon {
    background-image: url(data:image/png;base64,);
}

.answers .answer-wrong {
    background: #ff7d52!important;
    box-shadow: 0 5rpx 5rpx rgba(255,125,82,.2);
}

.answers .answer-wrong icon {
    background-image: url(data:image/png;base64,);
}

