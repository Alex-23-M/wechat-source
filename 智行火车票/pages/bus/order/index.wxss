@import "../../../__wuBaseWxss__/0.wxss";

.t_r {
    position: relative;
    text-align: right;
}

.order-info {
    font-size: 42rpx;
    line-height: 60rpx;
    padding: 40rpx 30rpx 10rpx;
    min-height: 120rpx;
    color: #FFF;
    background-color: #353439;
}

.flex {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}

.text_white {
    font-size: 0.8rem;
    white-space: nowrap;
    color: #FFF;
    line-height: 1.4rem;
    margin-top: 0.25rem;
}

.column-1 {
    -webkit-box-flex: 1;
    flex: 1;
}

.column-2 {
    -webkit-box-flex: 2;
    flex: 2;
}

.column-3 {
    -webkit-box-flex: 3;
    flex: 3;
}

.column-4 {
    -webkit-box-flex: 4;
    flex: 4;
}

.column-5 {
    -webkit-box-flex: 5;
    flex: 5;
}

.column-6 {
    -webkit-box-flex: 6;
    flex: 6;
}

.icon-single:after {
    content: "\E148";
}

page {
    font-size: 32rpx;
    color: #333;
    background-color: #EFEFEF;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

@font-face {
    font-family:'wechat_bus';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;
}

icon {
    font-family: "wechat_bus"!important;
    font-size: 42rpx;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    display: inline-block;
    vertical-align: middle;
}

.icon-warn:after {
    content: "\E022";
}

.warn_tips_top {
    position: fixed;
    top: -1rpx;
    left: 0;
    right: 0;
    text-align: center;
    font-size: 36rpx;
    line-height: 40rpx;
    padding: 20rpx 20rpx;
    opacity: 0.8;
    color: #FFF;
    background-color: #E64340;
}

.tips_top {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    text-align: center;
    font-size: 30rpx;
    line-height: 30rpx;
    padding: 30rpx 20rpx;
    color: #fff;
    background-color: #111;
    opacity: 0.65;
    z-index: 999;
}

.price {
    width: 12rpx;
    height: 12rpx;
    border-top: 4rpx solid #F3F3F3;
    border-right: 4rpx solid #F3F3F3;
    transform: rotate(45deg);
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -8rpx;
}

.small {
    font-size: 30rpx;
}

.refund-desc {
    font-size: 26rpx;
    color: rgba(255,255,255,0.6);
    padding-right: 48rpx;
    line-height: 48rpx;
}

.order-optional {
    background: #fff8de;
    padding: 20rpx 30rpx;
    font-size: 26rpx;
    color: #a76206;
}

.btn_default {
    color: #fff;
    background-color: #5495e6;
}

.businfo,.order-btn,.ticket-pick,.other-info {
    background-color: #FFF;
    padding: 30rpx 40rpx;
    margin-bottom: 20rpx;
}

.row {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}

.ticket-pick {
    margin-bottom: 0;
}

.businfo view,.ticket-pick view,.other-info {
    line-height: 56rpx;
    font-size: 32rpx;
    margin-top: 10rpx;
}

.other-info {
    margin-top: 0;
    border-top: 2rpx solid #F3F3F3;
}

.businfo .info-title,.ticket-pick .ticket-title {
    font-size: 42rpx;
    font-weight: 400;
}

.ticket-pick text,.other-title {
    display: inline-block;
    width: 160rpx;
    color: #999;
}

.clock {
    line-height: 20rpx;
    margin-right: 24rpx;
}

.icon-info,.icon-time,.icon-depart,.icon-dest {
    width: 60rpx;
    -webkit-text-stroke-width: 0.8rpx;
}

.icon-time:after {
    content: "\E11F";
}

.icon-depart:after {
    content: "\E11B";
}

.icon-dest:after {
    content: "\E11C";
}

.bus-order-qrcode {
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.bus-order-qrcode .code {
    width: 80%;
    text-align: center;
    border-bottom: 1px solid #F3F3F3;
}

.bus-order-qrcode text {
    margin: -20rpx 0 20rpx;
    display: block;
    text-align: center;
    width: 100%;
    color: #333;
}

.mt10 {
    margin-top: 20rpx;
}

.bus-order-qrcode image {
    width: 100%;
}

.txt {
    color: #999;
}

.qrcode-float {
    position: fixed;
    top: 0;
    bottom: 0;
    width: 100%;
    z-index: 99;
    background-color: #FFF;
}

.qr-content {
    position: absolute;
    width: 100%;
    text-align: center;
    top: 50%;
    -webkt-transform: translate(0,-50%);
    transform: translate(0,-50%);
}

.qr-content image {
    width: 100%;
}

.btuagain {
    width: 330rpx;
    height: 100rpx;
    line-height: 100rpx;
    margin-top: 14rpx;
    font-size: 30rpx;
}

.arr-right {
    position: relative;
}

.arr-right:after {
    width: .3rem;
    height: .3rem;
    border-top: 0.1rem solid #FFF;
    border-right: 0.1rem solid #FFF;
    transform: rotate(45deg);
    position: absolute;
    content: "";
    top: 50%;
    margin-top: -0.15rem;
}

.arr-right {
    right: 0;
    padding-right: .4rem;
}

.shadow {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
    background: rgba(0,0,0,0.7);
}

.priceDetail {
    margin: 4rem 1.5rem 0;
    z-index: 3000;
}