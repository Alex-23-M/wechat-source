@-webkit-keyframes light1 {
    0% {
        opacity: .4;
        transform: scale(1.1);
    }

    50% {
        opacity: .2;
        transform: scale(.9);
    }

    100% {
        opacity: .4;
        transform: scale(1.1);
    }
}

@keyframes light1 {
    0% {
        opacity: .4;
        transform: scale(1.1);
    }

    50% {
        opacity: .2;
        transform: scale(.9);
    }

    100% {
        opacity: .4;
        transform: scale(1.1);
    }
}

@-webkit-keyframes light2 {
    0% {
        opacity: .2;
        transform: scale(.9);
    }

    50% {
        opacity: .4;
        transform: scale(1.1);
    }

    100% {
        opacity: .2;
        transform: scale(.9);
    }
}

@keyframes light2 {
    0% {
        opacity: .2;
        transform: scale(.9);
    }

    50% {
        opacity: .4;
        transform: scale(1.1);
    }

    100% {
        opacity: .2;
        transform: scale(.9);
    }
}

@-webkit-keyframes bangzi {
    0% {
        transform: rotate(0);
    }

    50% {
        transform: rotate(30deg);
    }

    100% {
        transform: rotate(0);
    }
}

@keyframes bangzi {
    0% {
        transform: rotate(0);
    }

    50% {
        transform: rotate(30deg);
    }

    100% {
        transform: rotate(0);
    }
}

