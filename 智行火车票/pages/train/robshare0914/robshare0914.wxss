.speed-img__up {
    animation: rotate 1.5s linear infinite;
}

@-webkit-keyframes rotate {
    0%,100% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

@keyframes rotate {
    0%,100% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

@-webkit-keyframes translate {
    0%,100% {
        transform: translate(0,-3rpx);
    }

    25% {
        transform: translate(0,0);
    }

    50% {
        transform: translate(0,3rpx);
    }

    75% {
        transform: translate(0,0);
    }
}

@keyframes translate {
    0%,100% {
        transform: translate(0,-3rpx);
    }

    25% {
        transform: translate(0,0);
    }

    50% {
        transform: translate(0,3rpx);
    }

    75% {
        transform: translate(0,0);
    }
}

