.speed-open .tit {
    margin-left: 30rpx;
    font-size: 26rpx;
    color: #333;
}

.speed-open .tit.strong {
    font-size: 34rpx;
    font-weight: 600;
}

.speed-open-bd {
    padding: 10rpx 30rpx;
    overflow: hidden;
}

.speed-type {
    display: flex;
    line-height: 36rpx;
}

.speed-type .type {
    flex: 1;
    text-align: center;
    font-size: 26rpx;
    position: relative;
}

.speed-type .type.cur {
    color: #fc6e51;
}

.speed-type .type:first-child {
    flex: .5;
    text-align: left;
}

.speed-type .type:last-child {
    flex: .5;
    text-align: right;
}

.speed-type .type:first-child::after,.speed-type .type:last-child::after {
    display: none;
}

.speed-line {
    width: 100%;
    height: 10rpx;
    border-radius: 10rpx;
    background: #eee;
    margin: 24rpx 0 10rpx;
}

.speed-line-blue {
    height: 10rpx;
    background: #fc6e51;
    border-radius: 10rpx;
    position: relative;
}

.speed-line-blue .speed-line-btn {
    width: 60rpx;
    height: 34rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 60rpx 34rpx;
    position: absolute;
    top: -12rpx;
    right: 0;
    margin-right: -32rpx;
    border-radius: 34rpx;
    box-shadow: 0 3rpx 10rpx rgba(0,0,0,.25);
}

.speed-open .speen-txt {
    font-size: 26rpx;
    margin: 0 30rpx;
}

.speed-open .speen-txt text {
    color: #fe6b5b;
    font-size: 30rpx;
    margin: 0 12rpx;
}