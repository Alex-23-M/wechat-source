.btn_primary {
    color: #fff;
    background-color: #5495e6;
}

.btn_primary_hover {
    color: rgba(255,255,255,0.6);
    background-color: #5495e6;
}

.btn_default {
    color: #fff;
    background-color: #5495e6;
}

.btn_default_hover {
    color: rgba(255,255,255,0.6);
    background-color: rgb(9,187,7);
}

.row {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}

.column-1 {
    -webkit-box-flex: 1;
    flex: 1;
}

.column-2 {
    -webkit-box-flex: 2;
    flex: 2;
}

.column-3 {
    -webkit-box-flex: 3;
    flex: 3;
}

.column-4 {
    -webkit-box-flex: 4;
    flex: 4;
}

.column-5 {
    -webkit-box-flex: 5;
    flex: 5;
}

.column-6 {
    -webkit-box-flex: 6;
    flex: 6;
}

.column-3 text {
    opacity: 1;
    transition: opacity 0.2s ease-in-out;
}

.column-3 .hide {
    opacity: 0;
}

.grey {
    color: #999;
}

.orange {
    color: #5495e6;
}

.red {
    color: #ff4646;
}

.white_bg {
    background-color: #fff;
}

.fr {
    float: right;
}

.b_border {
    border-style: solid;
    border-image: url(data:image/gif;base64,R0lGODlhBQAFAPABANra2v///yH5BAUHAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
    border-width: 0;
}

.bottom-border-1 {
    border-bottom: 2rpx solid;
}

.right-arrow:after {
    position: absolute;
    width: 16rpx;
    height: 16rpx;
    border-top: 2rpx solid #ccc;
    border-right: 2rpx solid #ccc;
    transform: rotate(45deg);
    content: "";
    right: 16rpx;
    top: 50%;
    margin-top: -8rpx;
}

.tips_top {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    text-align: center;
    font-size: 30rpx;
    line-height: 30rpx;
    padding: 30rpx 20rpx;
    color: #fff;
    background-color: #111;
    opacity: 0.65;
    z-index: 999;
}

.color_warn {
    color: #F43530!important;
}

.color_current,.color_enable {
    color: #5495e6;
}

.inline-block {
    display: inline-block;
}

.touch-scroll {
    -webkit-overflow-scrolling: touch;
}

.warn_tips_top {
    position: fixed;
    top: -1rpx;
    left: 0;
    right: 0;
    text-align: center;
    font-size: 36rpx;
    line-height: 40rpx;
    padding: 20rpx 20rpx;
    opacity: 0.8;
    color: #FFF;
    background-color: #E64340;
}

.warn_tips_top icon {
    color: #FFF;
    font-size: 40rpx;
    vertical-align: bottom;
    -webkit-text-stroke-width: 0.4px;
    padding-right: 20rpx;
}

.btn-confirm {
    width: 90%;
    margin-top: 30rpx;
}

.btusty {
    color: #33aa33;
    background-color: #ffffff;
}

.align-center {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}